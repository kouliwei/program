#include "include.h"
 
 
 void Delay_1ms(void)
{
    __IO uint16_t index = 0;
    for(index = 10000;index != 0;index--)
    {
    }
}

void Delay_Nms(__IO uint16_t nCount)
{
    uint16_t i = 0;
    for(i = 0;i<nCount;i++)
    {
        Delay_1ms();
    }
}



